{
  "framework": null,
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "/api/vercel.ts",
      "use": "@vercel/node",
     
      "config": {
        "includeFiles": [
          
          "node_modules/@tonyrl/rand-user-agent/data/user-agents.json"
          
        ]
      }
      // --- config 对象结束 ---
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/api/vercel.ts"
    }
  ]
}
